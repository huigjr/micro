{
	"config": {
        "platform": {
            "php": "5.3.10"
        }
    },
    "name": "muldata/prototype",
    "description": "Muldata prototype framework",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Huig",
            "email": "huigjr@gmail.com"
        }
    ],
    "minimum-stability": "alpha",
    "require-dev": {
        "phpunit/phpunit": "*"
